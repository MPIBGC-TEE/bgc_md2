	issn = {1573-515X},
	journal = {Biogeochemistry},
	month = {Jun},
	number = {1},
	pages = {69--83},
	title = {How long do elements cycle in terrestrial ecosystems?},
	url = {https://doi.org/10.1007/s10533-018-0452-z},
	volume = {139},
