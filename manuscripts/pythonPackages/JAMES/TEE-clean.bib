	journal = {Journal of Geophysical Research: Biogeosciences},
	keywords = {Radiocarbon, open systems, pool models, carbon age, transit time, ecology},
	number = {3},
	pages = {e2021JG006673},
	title = {Probability distributions of radiocarbon in open linear compartmental systems at steady-state},
