	doi = {10.1111/nph.17239},
	eprint = {https://nph.onlinelibrary.wiley.com/doi/pdf/10.1111/nph.17239},
	journal = {New Phytologist},
	keywords = {neutral lipids, non-structural carbohydrates (NSC), starch, wood anatomy, wood carbon storage, wood traits},
	number = {1},
	pages = {139-154},
	title = {Starch and lipid storage strategies in tropical trees relate to growth and mortality},
	url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.17239},
	volume = {230},
	year = {2021},
	bdsk-url-1 = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.17239},
