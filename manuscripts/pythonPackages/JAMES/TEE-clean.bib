	doi = {10.1029/2018MS001360},
	eprint = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2018MS001360},
	journal = {Journal of Advances in Modeling Earth Systems},
	keywords = {carbon cycle, global change, ecosystem modeling, biogeochemical cycles, dynamical systems, compartmental systems},
	number = {8},
	pages = {1729-1734},
	postprint = {Sierra2018JAMES},
	title = {Representing and Understanding the Carbon Cycle Using the Theory of Compartmental Dynamical Systems},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2018MS001360},
	volume = {10},
	year = {2018},
